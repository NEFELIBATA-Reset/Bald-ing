import java.util.*;
public class Solution {
    public int NumberOf1Between1AndN_Solution(int n) {
        int count=0;
        while(n>0){
            String s=String.valueOf(n);
            char[] a=s.toCharArray();
            for(int i=0;i<a.length;i++){
                if(a[i]=='1'){
                    count++;
                }
            }
            n--;
        }
        return count;
    
    }
}
